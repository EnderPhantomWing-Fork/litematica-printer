{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "icon": "assets/litematica-printer/icon.png",
  "name": "${mod_name}",
  "description": "An extension for Litematica that adds the missing printer functionality.",
  "authors": [
    "aleksilassila",
    "zhaixianyu",
    "BlinkWhite"
  ],
  "contact": {
    "website": "${mod_homepage}",
    "homepage": "${mod_sources}",
    "sources": "${mod_sources}"
  },

  "license": "${mod_license}",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.aleksilassila.litematica.printer.LitematicaPrinterMod"
    ],
    "client": [
      "me.aleksilassila.litematica.printer.LitematicaPrinterMod"
    ],
    "modmenu": [
      "me.aleksilassila.litematica.printer.config.ModMenu"
    ]
  },
  "mixins": [
    "litematica-printer.mixins.json"
  ],

  "depends": {
    "minecraft": "${minecraft_dependency}",
    "malilib": ">=0.12.0",
    "litematica": ">=0.11.6",
    "fabric-content-registries-v0": "*"
  },
  "custom": {
    "modmenu": {
      "parent": "${mod_wrapper_id}"
    }
  }
}

